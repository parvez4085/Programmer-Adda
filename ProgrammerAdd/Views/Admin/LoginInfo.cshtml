@{
    ViewBag.Title = "LoginInfo";
    Layout = "~/Views/Shared/AdminZoneMasterLayout.cshtml";
}

<div class="row">
    <div class="col-4"></div>
    <div class="col-4" style="text-align:center;">
       <h3>Login Information</h3>
    </div>
    <div class="col-4"></div>
</div>

<div>
        <table class="table">
    <tr style="font-size:22px;background:#ffd800;">
        <th>Email Id</th>
        <th>user type</th>
         <th>Delete</th>
    </tr>
    @using ProgrammerAdd.Models;
    @{
        ConnectionManager cm = new ConnectionManager();
        string command = "select * from TBL_Login";
        System.Data.DataTable dt = cm.GetBulksRecord(command);
        if(dt.Rows.Count>0)
        {
            for(int i=0;i<dt.Rows.Count;i++)
            {
        <tr>
        <td>@dt.Rows[i][0].ToString()</td>
            <td>@dt.Rows[i][2].ToString()</td>
        <td><a href="/Admin/delete?dt=@dt.Rows[i][0].ToString()">Click</a></td>
      
       </tr>
            }
        }
    }
   
</table>
    </div>